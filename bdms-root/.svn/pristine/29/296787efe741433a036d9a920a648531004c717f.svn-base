package com.bdms.kafka.entity;

public class KafkaPropertyEntity {
	
	private String topic;
	private int threads;
	private int gap;
	private int id_random;
	private int interval;
	private int cache;
	
	public String getTopic() {
		return topic;
	}
	public void setTopic(String topic) {
		this.topic = topic;
	}
	public int getThreads() {
		return threads;
	}
	public void setThreads(int threads) {
		this.threads = threads;
	}
	public int getInterval() {
		return interval;
	}
	public void setInterval(int interval) {
		this.interval = interval;
	}

	public int getGap() {
		return gap;
	}
	public void setGap(int gap) {
		this.gap = gap;
	}
	public int getId_random() {
		return id_random;
	}
	public void setId_random(int id_random) {
		this.id_random = id_random;
	}
	
	public int getCache() {
		return cache;
	}
	public void setCache(int cache) {
		this.cache = cache;
	}
	
	@Override
	public String toString() {
		return "KafkaPropertyEntry [topic=" + topic + ", threads=" + threads
				+ ", gap=" + gap + ", id_random=" + id_random + ", interval=" + interval
				+ ", maxrecordcache=" + cache + "]";
	}
	
	
	

}
