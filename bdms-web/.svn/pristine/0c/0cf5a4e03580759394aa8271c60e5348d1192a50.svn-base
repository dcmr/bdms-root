<script type="text/javascript">
	var bnr_data = [
		{'title': '上海迪爱斯', 'image': '<c:url value="/style/images/ksh.png" />', 'url': '/web/help/index.html?item=chat_weixin'},
		{'title': '上海迪爱斯', 'image': '<c:url value="/style/images/qwjs.png" />'},
		{'title': '上海迪爱斯', 'image': '<c:url value="/style/images/banner_2013.png" />'}
	];

	function bnrImg(idx) {
		idx = typeof(idx) == 'undefined' || ! idx ? 0 : (idx >= bnr_data.length ? 0 : idx);
		var bnr = $('#bnr-img');
		if(!bnr || bnr.length < 1) {
			if(bnr_data.length < 1) {
				return;
			}else if(bnr_data.length == 1) { 
				$('.bnr-slider').append('<img id="bnr-img" alt="' + bnr_data[idx]['title'] + '" src="' + bnr_data[idx]['image'] + '" />');
				return;
			}

			$('.bnr-slider').append('<img id="bnr-img" alt="" src="" />');
			bnr = $('#bnr-img');
		}

		if($.browser.msie && (parseInt($.browser.version) == 7 || parseInt($.browser.version) == 8)) {
			bnr.attr('src', bnr_data[idx]['image']);
		}else {
			bnr.css('opacity', '0.1').attr('src', bnr_data[idx]['image']).attr('alt', bnr_data[idx]['title']).animate({
				opacity: '1'
			}, 2000).delay(4000).animate({
				opacity: '0.1'
			}, 2000);
		}
		if(bnr_data[idx]['url']) {
			bnr.css('cursor', 'pointer').unbind('click').bind('click', function() {
				// window.open(bnr_data[idx]['url']);
				location.href = bnr_data[idx]['url'];
			});
		}else {
			bnr.css('cursor', 'default').unbind('click');
		}

		setTimeout(function() {
			bnrImg(idx + 1);
		}, 8000);
	};

	bnrImg(0);
	
	// 引导提示
	if(! getCookie('tips_float')) {
		obj = $('#tips_float');
		setTimeout(function() {
			obj.slideDown(300);
		}, 2000);
		obj.children('div.register').unbind('click').bind('click', function() {
			location.href = "/member/index/register.html";
		}).end().children('div.close').unbind('click').bind('click', function() {
			setCookie("tips_float", 'close', null, null, '/');
			$('#tips_float').hide();
		});
	}	
</script>

